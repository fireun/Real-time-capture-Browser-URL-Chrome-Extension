{
  "manifest_version": 2,
  "name": "Browsing Monitor",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  }
}
